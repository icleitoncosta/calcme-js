{
  "name": "calcme-api",
  "version": "0.1.1",
  "private": false,
  "description": "Projeto open-source para exportar as funcoes do CalcMe sistemas para comunicacao visual, e poder chama-las remotamente",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "author": {
    "name": "Cleiton Carvalho",
    "email": "cleitoncostacarvalho2019@gmail.com",
    "url": "https://github.com/icleitoncosta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icleitoncosta/calcme-js"
  },
  "homepage": "https://calcme-community.github.io/",
  "exports": {
    ".": "./dist/index.js"
  },
  "keywords": [
    "brasil",
    "calcme",
    "graficas",
    "comunicacao visual",
    "calcme.com.br",
    "sistema de gestao",
    "api",
    "lib"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "shx rm -rf dist",
    "commit": "cz",
    "lint": "npx eslint --ext .ts src",
    "prepare": "husky install && npm run clean && npm run build",
    "release": "release-it",
    "test": "jest",
    "example:orders": "ts-node examples/get-orders",
    "example:auto-nf": "ts-node examples/emite-nfse-automaticamente",
    "docs:build": "npm run docs:clean && npm run docs:theme && typedoc --gitRevision main",
    "docs:clean": "shx rm -rf reference",
    "docs:theme": "cd ./docs-source && tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/debug": "^4.1.12",
    "@types/node": "^18.6.1",
    "@types/parse-data-url": "^3.0.0",
    "@types/prettier": "^2.7.3",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "buffer": "^6.0.3",
    "conventional-changelog-cli": "^2.2.2",
    "debug": "^4.3.4",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eventemitter2": "^6.4.9",
    "file-type": "~16.5.4",
    "fs": "^0.0.1-security",
    "jest": "^29.4.2",
    "node-fetch": "^3.3.0",
    "parse-data-url": "^4.0.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "release-it": "^15.11.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.25",
    "typedoc-plugin-mdn-links": "^2.0.2",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.5",
    "typescript-debounce-decorator": "^0.0.18",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "husky": "^8.0.3"
  }
}
